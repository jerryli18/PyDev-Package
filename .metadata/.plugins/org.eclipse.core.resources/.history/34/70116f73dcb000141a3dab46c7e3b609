def recognize(words):
    a = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"
    b = "NOPQRSTUVWXYZABCDEFGHIJKLMnopqrstuvwxyzabcdefghijklm"
    final = ""
    dictionary = open("../data/dictionary.txt")
    x = []
    for line in dictionary:
        x.append(line)
        
      
    for s in words:
        i = a.find(s)
        final = final + b[i]
    
    count = 0
    for word in words:
        if word.endswith("ay"):
            count += 1
    if count == len(words):
        def vowel(word):
            if "a" in word:
                return True
            if "e" in word:
                return True
            if "i" in word:
                return True
            if "o" in word:
                return True
            if "u" in word:
                return True
        if "way" in (word):
            return word[0:-4]
        elif word[-4] == "q":
            return "qu"+ word[0:-5]
        else:
            a = word.find("-")
            b = a+1
            return word[b:-2]+ word[0:a]
        return "unpigify"  
    elif final in x:
        return final
        return "un-rot13"
    else:
        return "none"
   

        